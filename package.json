{
  "name": "shopify-theme-lab",
  "description": "Customizable modular development environment for blazing-fast Shopify theme creation",
  "author": "Sergej Samsonenko",
  "version": "4.4.1",
  "license": "MIT",
  "scripts": {
    "config:dev": "shopify theme pull -e development-config",
    "config:live": "shopify theme pull -e live-config",
    "config:init": "shopify theme pull -e live-config && shopify theme push -e development-config",
    "config:update": "shopify theme pull -e store-config && shopify theme push -e store-config",
    "content:init": "shopify theme pull -e live-content && shopify theme push -e development-content",
    "content:update": "shopify theme pull -e store-content && shopify theme push -e store-content",
    "deploy": "run-s webpack:build && shopify theme push -e store",
    "deploy:new": "run-s webpack:build && shopify theme push -e store --unpublished",
    "dev": "npm run start",
    "dev:sync": "npm run start:sync",
    "fix": "run-s -c fix:*",
    "fix:js": "eslint src/**/*.{js,vue} --config .config/.eslintrc.js --fix",
    "fix:css": "stylelint src/**/*.{vue,css,sass,scss} --config .config/.stylelintrc.js --fix",
    "fix:shopify": "shopify theme check -a --e store",
    "lint": "run-s -c lint:*",
    "lint:js": "eslint src/**/*.{js,vue} --config .config/.eslintrc.js",
    "lint:css": "stylelint src/**/*.{vue,css,sass,scss} --config .config/.stylelintrc.js",
    "lint:shopify": "shopify theme check -e store",
    "login": "npm run themes",
    "metafields": "shopify theme metafields pull -e store",
    "pull": "shopify theme pull -e store",
    "pull:content": "shopify theme pull -e store-content",
    "pull:content:dev": "shopify theme pull -e development-content",
    "pull:content:live": "shopify theme pull -e live-content",
    "pull:content:deploy": "shopify theme pull -e deploy-content",
    "pull:dev": "shopify theme pull -e development",
    "pull:live": "shopify theme pull -e live",
    "push": "npm run deploy",
    "push:content": "shopify theme push -e store-content",
    "push:content:dev": "shopify theme push -e development-content",
    "push:content:deploy": "shopify theme push -e deploy-content",
    "push:new": "npm run deploy:new",
    "shopify:dev": "shopify theme dev -e store",
    "shopify:dev:sync": "shopify theme dev -e store --theme-editor-sync",
    "shopify:login": "npm run themes",
    "shopify:pull": "npm run pull",
    "shopify:pull:dev": "npm run pull:dev",
    "shopify:pull:live": "npm run pull:live",
    "shopify:push": "npm run push",
    "shopify:push:new": "npm run push:new",
    "shopify:settings:dev": "npm run config:dev",
    "shopify:settings:init": "npm run config:init",
    "shopify:settings:live": "npm run config:live",
    "shopify:settings:update": "npm run config:update",
    "shopify:start": "npm run start",
    "shopify:start:sync": "npm run start:sync",
    "shopify:themes": "npm run themes",
    "start": "run-p -sr shopify:dev webpack:watch",
    "start:sync": "run-p -sr shopify:dev:sync webpack:watch",
    "themes": "shopify theme list -e store && shopify theme info -e store",
    "webpack:watch": "cross-env NODE_ENV=development BROWSERSLIST_ENV=development BROWSERSLIST_CONFIG=.config/.browserslistrc webpack --config .config/webpack/webpack.dev.js --watch --progress",
    "webpack:build": "cross-env NODE_ENV=production BROWSERSLIST_ENV=production BROWSERSLIST_CONFIG=.config/.browserslistrc webpack --config .config/webpack/webpack.prod.js --progress"
  },
  "dependencies": {
    "tailwindcss": "^3.0.2",
    "vue": "^3.2.26",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@vue/compiler-sfc": "^3.2.26",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "lazysizes": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^16.8.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
